<h1>Add New Person</h1> 

<%= form_for @person do |p| %>
  <%= p.label :first_name %>
  <%= p.text_field :first_name %>
  <br />
  <%= p.label :last_name %>
  <%= p.text_field :last_name %>
  <br />
  <%= p.label :age %>
  <%= p.text_field :age %>
  <br />
  <%= p.label :gender %>
  <%= p.text_field :gender %>
  <br />
  <%= p.label :alive %>
  <%= p.text_field :alive %>
  <br />
  <%= p.label :relative%>
  <%= p.text_field :relative %>
  <br />
  <%= p.submit %> 
  <% end %> 